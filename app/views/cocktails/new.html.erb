<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to "#", class: "navbar-brand" do %>
    <%= image_tag "data:image/jpeg;base64," %>
  <% end %>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <%= link_to "Go back to cocktails' list", cocktails_path, class: "nav-link" %>
      </li>
    </ul>
  </div>
</div>

<div class="banner" id="banner-cocktail" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://images.pexels.com/photos/613037/pexels-photo-613037.jpeg?auto=compress&cs=tinysrgb&dpr=2&w=500);">
  <div class="banner-content" id="">
    <h2>New cocktail</h2>
  </div>
</div>

<div class="container">
  <div class="form">
  <%= simple_form_for(@cocktail) do |f| %>
    <%= f.input :name %>
    <%= f.input :photo_url %>
    <%= f.submit "Create your cocktail", class: "btn btn-flat" %>
  <% end %>
  </div>
</div>
